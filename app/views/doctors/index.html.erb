<%= link_to "New Doctor", new_doctor_path %>
<h1> All Doctors </h1>

<% @doctors.each do |doctor| %>

<%= doctor.name %>
<%= doctor.type %>
<%= doctor.age %>

<%= link_to "Edit Doctor", edit_doctor_path(doctor) %>
<%= link_to "Show Doctor", doctor_path(doctor) %>
<%= link_to "Delete", doctor_path(doctor), method: :delete, data: {confirm: "Are you sure you want to delete"}%>
<% end %>
